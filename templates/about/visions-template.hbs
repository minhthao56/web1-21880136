<h1 class="mb-3 text-black-40">Our Vision</h1>
{{#each data}}
  <a
    href="?req=visions/{{id}}"
    class="clearfix mb-2 text-decoration-none"
  >
    <img
      src="{{thumbpath}}"
      alt="{{thumbpath}}"
      class="float-start me-3 rounded-4"
    />
    <h3 class="text-black">{{name}}</h3>
    <p class="text-black-50">
      {{summary}}
    </p>
  </a>
{{/each}}